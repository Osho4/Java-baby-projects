class ThrowsDemo
 {
     static void throwOne()
     {
        System.out.println("inside throwOne");
        throw new IllegalAccessException("demo");
        }
        
        public static void main(String args[])
        {
            try{
            throwOne();
        }
        catch(IllegalAccessException e){
            System.out.println("caught:"+e);
            }
        }
    }
